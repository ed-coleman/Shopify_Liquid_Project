<h1>{{ section.settings.title }}</h1>
<br>
<h2>{{ section.settings.subtext_title }}</h2>

{% schema %}
  {
    "name": "Template 404",
    "settings": [
      {
        "type": "text",
        "id": "title",
        "default": "404",
        "label": "404 Heading Title"
      }, {
        "type": "text",
        "id": "subtext_title",
        "default": "The page you are looking for does not exist",
        "label": "404 Subtext Title"
      }
    ]
  }
{% endschema %}